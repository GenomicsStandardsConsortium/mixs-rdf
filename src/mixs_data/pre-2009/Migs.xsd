<?xml version="1.0" encoding="utf-8"?>
<!-- edited with XMLSpy v2007 sp2 (http://www.altova.com) by Kelvin LI (J. Craig Venter Institute) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:annotation>
        <xs:documentation xml:lang="en">
            This schema is the Genomic Standards Consortium implementation of the MIGS checklist (Nat Biotech in review).
            The latest version of the schema can always be found at : http://gensc.sf.net
            Last update: Nov 13, 2006
        </xs:documentation>
    </xs:annotation>
    <xs:attribute name="group"/>
    <xs:element name="migs">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="genome_catalogue">
                    <xs:annotation>
                        <xs:documentation>The information  in genome catalogue is not part of MIGS.  It is common to all entries and is required to give each genome report a unique ID and some descriptive information that links it to external information.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="gcat_identifier" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Please enter your GCat identifier if you have one. GCat identifiers are now being issued for the sake of publication. GCat identifiers can be requested from curator@ceh.ac.uk. Please see GSC documentation on requesting GCat identifiers. Look at the GSC wiki at http://gensc.sf.net.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:attribute name="group" fixed="111111"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="genome_report_title">
                                <xs:annotation>
                                    <xs:documentation>A brief (3-4 word) title for this genome that will appear in the genome catalogue in the absence of any other identifying data (i.e. especially important for metagenomes which will not have taxid or organism identifiers, and also important for incomplete genome reports)</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:string"/>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="taxonomic_group">
                                <xs:annotation>
                                    <xs:documentation>One of the taxonomic groupings recognized by MIGS.  Could be expanded in the future to further refine the ability of the MIGS specification to describe particular genomes. Input: CV</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="taxonomic_group_restriction"/>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="genome_project_id" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Please enter the Genome Project Identifier for your genome  (if available from the INSDC).</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:attribute name="group" fixed="110001"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="taxid" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Taxid imported from the NCBI Genome Projects database.  If your organism does not yet have a taxid one will be created the first time you submit a molecular sequence to Genbank.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:string">
                                            <xs:attribute name="group" fixed="111110"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="accession" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>The accession number to resolve all replicons in this entry in the INSDC databases (DDBJ/EMBL/Genbank)</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:attribute ref="group" fixed="111111"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="relevance">
                                <xs:annotation>
                                    <xs:documentation>A brief phrase or sentence describing the primary reason(s) this genome/metagenome was sequenced.  For example because it causes a particular disease, it is involved in a particular industrial process, or because this lineage has never been sampled before etc.  Input: free text</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:string">
                                            <xs:attribute name="group" fixed="111111"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="ncbi_organism_name" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Organism name imported from the INSDC Genome Project database.   If your entry does not have an organism name please register it with the INSDC.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:string">
                                            <xs:attribute name="group" fixed="110000"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="ncbi_status" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Status of project imported from NCBI Genome Projects database.  If your entry does not have a status please register it with NCBI.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:string">
                                            <xs:attribute name="group" fixed="110000"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="gold_stamp" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Identifier imported from the Genomes Online Database.  If your entry does not have one, please register at GOLD.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:string">
                                            <xs:attribute name="group" fixed="110001"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute name="group" fixed="111111"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Investigation">
                    <xs:annotation>
                        <xs:documentation>Investigation as defined by RSBI and supported by MICheck http://micheck.sf.net.  An investigation in MIGS refers to the general of a single genome or metagenomic dataset.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Study">
                                <xs:annotation>
                                    <xs:documentation>Study as defined by RSBI and supported by MICheck http://micheck.sf.net</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Biomaterial" maxOccurs="unbounded">
                                            <xs:annotation>
                                                <xs:documentation>Any study can have one or more Biomaterial elements, each Biomaterial element defines a distinct sampling event.</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element ref="Biosample" maxOccurs="unbounded">
                                                        <xs:annotation>
                                                            <xs:documentation>One or more Biosample elements may be derived from a given Biomaterial, having optionally applied any number of assay elements to the Biomaterial using the AssayMaterial element. </xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element ref="environment"/>
                                                    <xs:choice minOccurs="0">
                                                        <xs:element name="sediment">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element name="sediment_depth">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="currents">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="incident_light">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="porosity">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="permeabillity">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="grain_size_distribution">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                                <xs:attribute ref="group" fixed="000001"/>
                                                            </xs:complexType>
                                                        </xs:element>
                                                        <xs:element name="sediment_pore_water">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element name="alkalinity">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="dissolved_oxygen">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="eH">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="major_cations">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="major_anions">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="nitrogen_speciation">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="phosphorus_speciation">
                                                                        <xs:complexType>
                                                                            <xs:attribute ref="group" fixed="000001"/>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                                <xs:attribute ref="group" fixed="000001"/>
                                                            </xs:complexType>
                                                        </xs:element>
                                                        <xs:element name="water_body">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element name="temperature" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="pH" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="salinity" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="pressure" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="chlorophyl" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="conductivity" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="light_intensity" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="DOC" type="measurementType" minOccurs="0">
                                                                        <xs:annotation>
                                                                            <xs:documentation>dissolved organic carbon</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="current" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="atmospheric_data" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="density" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="alkalinity" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="dissolved_oxygen" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="POC" type="measurementType" minOccurs="0">
                                                                        <xs:annotation>
                                                                            <xs:documentation>particulate organic carbon</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="phosphate" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="nitrate" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="sulphates" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="sulphides" type="measurementType" minOccurs="0"/>
                                                                    <xs:element name="primary_production" type="measurementType" minOccurs="0"/>
                                                                </xs:sequence>
                                                                <xs:attribute ref="group" fixed="000001"/>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:choice>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element ref="Assay"/>
                                        <xs:element name="phenotype">
                                            <xs:annotation>
                                                <xs:documentation>The information in phenotype describes information about physical characteristics and other traits.</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="propagation">
                                                        <xs:annotation>
                                                            <xs:documentation>Specific to different taxa. Phage: lytic/lysogenic Plasmids: incompatibility group. Input: CV</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:simpleContent>
                                                                <xs:extension base="propagation_restriction"/>
                                                            </xs:simpleContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="encoded_traits" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:annotation>
                                                            <xs:documentation>Should include key traits like antibiotic resistance or xenobiotic degradation phenotypes for plasmids, converting genes for phage. This is an example of a generic definition that users can customize as any number of traits could be caputured if this field were substructred.  In the future, this could be selected from an ontology, and traits like, antibiotic resistence could be enumerated with any number of specific types of antibiotics. Input: CV</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:simpleContent>
                                                                <xs:extension base="encoded_traits_restriction">
                                                                    <xs:attribute ref="group" fixed="001100"/>
                                                                </xs:extension>
                                                            </xs:simpleContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="relationship_to_oxygen">
                                                        <xs:annotation>
                                                            <xs:documentation>Is this organism an aerobe, anaerobe etc</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:simpleContent>
                                                                <xs:extension base="relationship_to_oxygen_restriction">
                                                                    <xs:attribute ref="group" fixed="010000"/>
                                                                </xs:extension>
                                                            </xs:simpleContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="group" fixed="111111"/>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="sources">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="ncbi_genome_projects_prokaryotes" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="ncbi_taxonomic_group"/>
                                                                <xs:element name="ncbi_sequence_status"/>
                                                                <xs:element name="ncbi_genome_size"/>
                                                                <xs:element name="ncbi_gc_content"/>
                                                                <xs:element name="ncbi_gram_stain"/>
                                                                <xs:element name="ncbi_shape"/>
                                                                <xs:element name="ncbi_arrangement"/>
                                                                <xs:element name="ncbi_endospore"/>
                                                                <xs:element name="ncbi_motility"/>
                                                                <xs:element name="ncbi_salinity"/>
                                                                <xs:element name="ncbi_oxygen"/>
                                                                <xs:element name="ncbi_habitat"/>
                                                                <xs:element name="ncbi_temperature_range"/>
                                                                <xs:element name="ncbI_pathogen"/>
                                                                <xs:element name="ncbi_disease"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="gold" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="gold_organism"/>
                                                                <xs:element name="gold_strain"/>
                                                                <xs:element name="gold_serovar"/>
                                                                <xs:element name="gold_culture"/>
                                                                <xs:element name="gold_symbiont"/>
                                                                <xs:element name="gold_type"/>
                                                                <xs:element name="gold_phylogeny"/>
                                                                <xs:element name="gold_status"/>
                                                                <xs:element name="gold_phenotype"/>
                                                                <xs:element name="gold_relevance"/>
                                                                <xs:element name="gold_disease"/>
                                                                <xs:element name="gold_ecotype"/>
                                                                <xs:element name="gold_isolatiion"/>
                                                                <xs:element name="gold_country"/>
                                                                <xs:element name="gold_availabiity"/>
                                                                <xs:element name="gold_size">
                                                                    <xs:annotation>
                                                                        <xs:documentation>In kb</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                                <xs:element name="gold_orfs"/>
                                                                <xs:element name="gold_data"/>
                                                                <xs:element name="gold_institute"/>
                                                                <xs:element name="gold_funding"/>
                                                                <xs:element name="gold_publication"/>
                                                                <xs:element name="gold_date"/>
                                                                <xs:element name="gold_contact_name"/>
                                                                <xs:element name="gold_contact_email"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute ref="group" fixed="000000"/>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="group" fixed="111111"/>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="entity_data_collection">
        <xs:sequence>
            <xs:element name="entity" type="xs:string"/>
            <xs:element name="measurement" type="xs:string"/>
            <xs:element name="value" type="xs:string"/>
            <xs:element name="unit" type="xs:string"/>
            <xs:element name="assay" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="taxonomic_group_attribute">
        <xs:simpleContent>
            <xs:extension base="taxonomic_group_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="nucleic_acid_extraction_attribute">
        <xs:simpleContent>
            <xs:extension base="nucleic_acid_extraction_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="vector_attribute">
        <xs:simpleContent>
            <xs:extension base="vector_restriction">
                <xs:attribute name="group" fixed="000001"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="sampling_strategy_attribute">
        <xs:simpleContent>
            <xs:extension base="sampling_strategy_restriction">
                <xs:attribute name="group" fixed="000001"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="reason_for_sequencing_attribute">
        <xs:simpleContent>
            <xs:extension base="reason_for_sequencing_restriction">
                <xs:attribute name="group" fixed="000001"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="status_of_isolate_attribute">
        <xs:simpleContent>
            <xs:extension base="status_of_isolate_restriction">
                <xs:attribute name="group" fixed="010000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="ncbi_status_attribute">
        <xs:simpleContent>
            <xs:extension base="ncbi_status_restriction">
                <xs:attribute name="group" fixed="010000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="direction_longitude_attribute">
        <xs:simpleContent>
            <xs:extension base="direction_longitude_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="collection_attribute">
        <xs:simpleContent>
            <xs:extension base="collection_restriction">
                <xs:attribute name="group" fixed="110010"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="direction_latitude_attribute">
        <xs:simpleContent>
            <xs:extension base="direction_latitude_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="assembly_method_attribute">
        <xs:simpleContent>
            <xs:extension base="assembly_method_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="specific_host_classification_attribute">
        <xs:simpleContent>
            <xs:extension base="specific_host_classification_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="relationship_attribute">
        <xs:simpleContent>
            <xs:extension base="relationship_restriction">
                <xs:attribute name="group" fixed="100000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="is_type_strain_attribute">
        <xs:simpleContent>
            <xs:extension base="is_type_strain_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="status_of_host_attribute">
        <xs:simpleContent>
            <xs:extension base="status_of_host_restriction"/>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="environment_attribute">
        <xs:simpleContent>
            <xs:extension base="environment_restriction">
                <xs:attribute name="group" fixed="011000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="propagation_attribute">
        <xs:simpleContent>
            <xs:extension base="propagation_restriction">
                <xs:attribute name="group" fixed="101100"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="trophic_level_attribute">
        <xs:simpleContent>
            <xs:extension base="trophic_level_restriction">
                <xs:attribute name="group" fixed="110000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="relationship_to_oxygen_attribute">
        <xs:simpleContent>
            <xs:extension base="relationship_to_oxygen_restriction">
                <xs:attribute name="group" fixed="010000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="metabolic_strategy_attribute">
        <xs:simpleContent>
            <xs:extension base="metabolic_strategy_restriction">
                <xs:attribute name="group" fixed="010000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="biotic_relationship_attribute">
        <xs:simpleContent>
            <xs:extension base="biotic_relationship_restriction">
                <xs:attribute name="group" fixed="010000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="spore_forming_attribute">
        <xs:simpleContent>
            <xs:extension base="spore_forming_restriction">
                <xs:attribute name="group" fixed="010000"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="dna_amplification_attribute">
        <xs:simpleContent>
            <xs:extension base="dna_amplification_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="pathogenicity_attribute">
        <xs:simpleContent>
            <xs:extension base="pathogenicity_restriction">
                <xs:attribute name="group" fixed="000100"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="publication_source_attribute">
        <xs:simpleContent>
            <xs:extension base="publication_source_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="encoded_traits_attribute">
        <xs:simpleContent>
            <xs:extension base="encoded_traits_restriction">
                <xs:attribute name="group" fixed="001100"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="habitat_type_attribute">
        <xs:simpleContent>
            <xs:extension base="habitat_type_restriction">
                <xs:attribute name="group" fixed="111111"/>
            </xs:extension>
